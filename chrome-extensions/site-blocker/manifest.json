{
  "manifest_version": 3,
  "name": "Site Blocker",
  "version": "1.0",
  "description": "Блокирует сайты по списку",
  "permissions": ["storage", "declarativeNetRequest"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}